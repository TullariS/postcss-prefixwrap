{
  "name": "postcss-prefixwrap",
  "version": "1.5.1",
  "description": "A PostCSS plugin that is used to wrap css styles with a css selector to constrain their affect on parent elements in a page.",
  "license": "MIT",
  "author": "Daniel Tedman (https://danieltedman.com)",
  "repository": "https://github.com/dbtedman/postcss-prefixwrap",
  "keywords": [
    "css",
    "javascript",
    "nodejs",
    "npm",
    "postcss",
    "postcss-plugin"
  ],
  "main": "./src/main.js",
  "scripts": {
    "format": "prettier ./**/*.{md,yml,yaml,json} --write && prettier-eslint ./**/*.js --write",
    "test": "npm run test:unit && npm run test:integration && npm run test:acceptance",
    "test:acceptance": "nyc --reporter=text --reporter=lcov mocha --colors ./acceptance/**/*Test.js",
    "test:integration": "npm run test:integration:prep && mocha --colors ./integration/**/*Test.js",
    "test:integration:prep": "gulp --gulpfile=./integration/fixtures/Gulpfile.js css",
    "test:unit": "nyc --reporter=text --reporter=lcov mocha --colors ./unit/**/*Test.js"
  },
  "peerDependencies": {
    "postcss": "*"
  },
  "devDependencies": {
    "@types/mocha": "5.2.6",
    "ajv": "6.10.0",
    "ajv-keywords": "3.4.0",
    "codecov": "3.3.0",
    "eslint": "5.16.0",
    "fs-extra": "7.0.1",
    "gulp": "4.0.1",
    "gulp-postcss": "8.0.0",
    "mocha": "6.1.4",
    "nyc": "14.0.0",
    "postcss": "7.0.14",
    "prettier": "1.17.0",
    "prettier-eslint-cli": "4.7.1"
  }
}
